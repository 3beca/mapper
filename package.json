{
  "name": "tribeca-mapper",
  "version": "0.0.1",
  "description": "A simple request mapper",
  "main": "build/src/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "cd build/src && node main.js",
    "start-dev": "cd src && ts-node main.ts --inspect=0.0.0.0:9229",
    "start-watch": "nodemon -L src/main.ts --inspect=0.0.0.0:9229 --exec ts-node",
    "lint": "eslint . --ext .ts",
    "test": "jest --watchAll",
    "test-ci": "jest --collect-coverage",
    "publish-code-coverage": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3beca/mapper.git"
  },
  "keywords": [
    "mapper",
    "nodejs"
  ],
  "author": "juanjofp, francescorivola",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3beca/mapper/issues"
  },
  "homepage": "https://github.com/3beca/mapper#readme",
  "devDependencies": {
    "@types/convict": "^5.2.1",
    "@types/jest": "^26.0.4",
    "@types/mongodb": "^3.5.25",
    "@types/node": "^14.0.23",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "codecov": "^3.7.0",
    "eslint": "^6.8.0",
    "eslint-plugin-filenames": "^1.3.2",
    "jest": "^25.5.4",
    "nock": "^13.0.2",
    "nodemon": "^2.0.4",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "convict": "^6.0.0",
    "fastify": "^3.0.2",
    "fastify-formbody": "^4.0.3",
    "fastify-swagger": "^3.1.1",
    "liquidjs": "^9.14.1",
    "mongodb": "^3.5.9",
    "node-fetch": "^2.6.0"
  }
}
